program testForBreakAndContinue;
var a,b,c : integer
var h : real

begin
    print("testing Continue");
    while ( a < 20) do
    begin
        a = a+1;
        if (( a == 10) or ( (a mod 2) == 1)) then
            begin
            continue
            end;
        print(a)
    end;
    a = 0;
    print("testing Break");

    while ( a < 20) do
    begin
        a = a+1;
        if ( a == 10)  then
            begin
            break
            end;
        print(a)
    end

end.
