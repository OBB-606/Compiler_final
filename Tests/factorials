program Factorials;
var a,b,c : integer
var h : real

func factorial (a: integer) return integer;
   var result,c,d,e : integer
   begin
       result = 1;
       e = 1;
       while ( e <= a ) do
       begin
            result = result * e;
            e = e + 1
       end;
       factorial = result
   end;

begin
    print("factorials");
    b = 10;
    a = 1;
    while ( a <= b) do
    begin
        c = factorial(a);
        print(c);
        a = a+1
    end

end.