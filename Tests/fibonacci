program Fibonacci;
var a,b,c : integer
var h : real

func Fib (a: integer) return integer;
   var num,c,d,e : integer
   begin
       c = 1;
       d = 1;
       while (num < a) do
       begin
            print(d);
            e = c;
            c = c + d;
            d = e;
            num = num + 1
       end
       Fib = num
   end;

begin
    print("the first a numbers of the Fibonacci sequence");
    a = Fib(10);

end.